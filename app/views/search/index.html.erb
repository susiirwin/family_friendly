<div class="container">
  <h1>Search Results</h1>
  <div class="jumbotron">
    <div class="results">
      <% @results.each do |b| %>
      <h3><%= b.name %>  <%= link_to "See Details", business_path(b.yelp_id), class: "btn btn-family" %></h3>
      Categories: <%= b.categories %>

      <table class="striped">
        <tr>
          <td>
            <p><%= b.address %></p>
          </td>
        </tr>


      </table>
      <br />
      <% end %>
    </div>
  </div>
</div>
